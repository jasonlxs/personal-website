/* pages/list/list.wxss */
.list{
     height: 100%;
     display:flex;
     flex-direction: column;
     padding:30rpx;
}
 navigator{overflow:hidden;}

.list-item{
    padding:20rpx;
    border-bottom: solid 1rpx #aaa;
}
.list-img{
    display:block;
    width:100px;
    height:100px;
    float:left;
}
.list-title{
     display: block;
     float:left;
     margin:20rpx;
     font-size:20px;
}
.list-time{
     display: block;
     float:left;
     margin:20rpx;
     font-size:16px;
     color:#aaa;
}

